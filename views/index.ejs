<%- include('./partials/header') %>
<div class="container-fluid d-flex flex-column align-items-center justify-content-center">

  <!-- logo -->
  <div class="row">
    <div class="col-lg-4 col-md-6 col-sm-12 offset-lg-4 text-center mb-4">
      <img src="/images/logo-1.png" alt="Logo 1" class="img-fluid">
    </div>
  </div>
  
  <!-- Video -->
  <div class="row justify-content-center mb-4">
    <div class="col-12">
      <div class="d-flex justify-content-center">
        <iframe width="720" height="315" src="https://www.youtube.com/embed/LAUi8pPlcUM" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" allowfullscreen></iframe>
      </div>
    </div>
  </div>
  
  <!-- Part two - Description with Photos -->

  <!-- image 1 -->
  <div class="row justify-content-center d-flex justify-content-center align-items-center">
    <div class="col-lg-8 col-md-10 col-sm-12">
      <div class="row mb-4">
        <div class="col-lg-6 col-md-10 col-sm-12 d-flex align-items-center">
          <img src="/images/img-1.png" alt="Photo 1" class="img-fluid" style="max-width: 350px; height: auto;">
        </div>
        <div class="col-lg-6 col-md-10 col-sm-12 d-flex align-items-center">
          <div class="mx-auto">
            <p class="text-center">Our website is designed to help GA college students who are struggling with coding or any step in programming</p>
          </div>
        </div>
      </div>
      <div class="row mb-4">
        <div class="col-lg-6 col-md-10 col-sm-12 d-flex align-items-center">
          <div class="mx-auto">
            <p class="text-center">Students can post their coding-related questions or problems on the website and get help from other students.</p>
          </div>
        </div>
        <div class="col-lg-6 col-md-10 col-sm-12 d-flex align-items-center">
          <img src="/images/img-2.png" alt="Photo 2" class="img-fluid" style="max-width: 350px; height: auto;">
        </div>
      </div>
      <div class="row mb-4">
        <div class="col-lg-6 col-md-10 col-sm-12 d-flex align-items-center">
          <img src="/images/img-3.png" alt="Photo 3" class="img-fluid" style="max-width: 350px; height: auto;">
        </div>
        <div class="col-lg-6 col-md-10 col-sm-12 d-flex align-items-center">
          <div class="mx-auto">
            <p class="text-center">The website features a chat thread where students can interact and exchange ideas to solve coding problems.</p>
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col-lg-6 col-md-10 col-sm-12 d-flex align-items-center">
          <div class="mx-auto">
            <p class="text-center">Our website aims to foster a collaborative learning environment and help students improve their coding skills.</p>
          </div>
        </div>
        <div class="col-lg-6 col-md-10 col-sm-12 d-flex align-items-center">
          <img src="/images/img-4.png" alt="Photo 4" class="img-fluid" style="max-width: 350px; height: auto;">
        </div>
      </div>
    </div>
  </div>
</div>

<%- include('./partials/footer') %>

